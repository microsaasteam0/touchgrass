{
  "version": 2,
  "name": "touchgrass",
  "public": true,
  "alias": ["touchgrass.entrext.in"],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "env": {
    "VITE_API_URL": "https://touchgrass-7.onrender.com/api",
    "VITE_SITE_URL": "https://touchgrass.entrext.in"
  }
}
